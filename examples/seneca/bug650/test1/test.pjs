PImage tex;
float r = 0;
void cb(){
  println('f');
  println(hex(tex.get(10,10)));
}

void setup(){
  size(640, 360,OPENGL);
  tex = loadImage("crate.jpg","",cb);

//  var o = tex.get(10,30);
  
   //textureMode(NORMALIZED);
}

void draw(){
background(0);
translate(width/2, height/2, 0);
fill(128,128,0);
rotateY(r+=0.01);
box(70);
}

